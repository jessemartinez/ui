    ".csc-work-creatorGroup": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "type": "fluid",
                "options": {
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-work-creator": {
                                    "decorators": [
                                        {
                                            "func": "cspace.termList",
                                            "type": "fluid",
                                            "options": {
                                                "root": "{row}",
                                                "elPath": "creator",
                                                "termListType": "creator"
                                            }
                                        }
                                    ]
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    },
                    "elPath": "creatorGroup",
                    "hidePrimary": true
                }
            }
        ]
    },
