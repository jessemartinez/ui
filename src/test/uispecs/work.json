                                "tree": {
                                    ".csc-work-useFor": {
                                        "decorators": [
                                            {
                                                "func": "cspace.autocomplete",
                                                "type": "fluid",
                                                "options": {
                                                    "queryUrl": "../../../tenant/core/vocabularies/work/autocomplete/useFor",
                                                    "vocabUrl": "../../../tenant/core/vocabularies/work/source-vocab/useFor"
                                                }
                                            }
                                        ],
                                        "value": "${{row}.useFor}"
                                    }
