        ".csc-work-workDateGroup": {
            "decorators": [
                {
                    "func": "cspace.structuredDate",
                    "type": "fluid",
                    "options": {
                        "elPath": "fields.dateDisplayDate",
                        "elPaths": {
                            "dateEarliestSingleQualifier": "fields.workDateGroup.dateEarliestSingleQualifier",
                            "scalarValuesComputed": "fields.workDateGroup.scalarValuesComputed",
                            "dateLatestDay": "fields.workDateGroup.dateLatestDay",
                            "dateLatestYear": "fields.workDateGroup.dateLatestYear",
                            "dateAssociation": "fields.workDateGroup.dateAssociation",
                            "dateEarliestSingleEra": "fields.workDateGroup.dateEarliestSingleEra",
                            "dateDisplayDate": "fields.workDateGroup.dateDisplayDate",
                            "dateEarliestSingleCertainty": "fields.workDateGroup.dateEarliestSingleCertainty",
                            "dateLatestEra": "fields.workDateGroup.dateLatestEra",
                            "dateEarliestSingleQualifierValue": "fields.workDateGroup.dateEarliestSingleQualifierValue",
                            "dateLatestCertainty": "fields.workDateGroup.dateLatestCertainty",
                            "dateEarliestSingleYear": "fields.workDateGroup.dateEarliestSingleYear",
                            "dateLatestQualifier": "fields.workDateGroup.dateLatestQualifier",
                            "dateLatestQualifierValue": "fields.workDateGroup.dateLatestQualifierValue",
                            "dateEarliestSingleQualifierUnit": "fields.workDateGroup.dateEarliestSingleQualifierUnit",
                            "datePeriod": "fields.workDateGroup.datePeriod",
                            "dateEarliestScalarValue": "fields.workDateGroup.dateEarliestScalarValue",
                            "dateLatestMonth": "fields.workDateGroup.dateLatestMonth",
                            "dateNote": "fields.workDateGroup.dateNote",
                            "dateLatestScalarValue": "fields.workDateGroup.dateLatestScalarValue",
                            "dateLatestQualifierUnit": "fields.workDateGroup.dateLatestQualifierUnit",
                            "dateEarliestSingleDay": "fields.workDateGroup.dateEarliestSingleDay",
                            "dateEarliestSingleMonth": "fields.workDateGroup.dateEarliestSingleMonth"
                        }
                    }
                }
            ],
            "value": "${fields.dateDisplayDate}"
        },
        ".csc-work-historyNotes-label": {
