        },
        ".csc-work-shortIdentifier": "${fields.shortIdentifier}",
        ".csc-work-indexingNotes-label": {
            "messagekey": "work-indexingNotesLabel"
        },
        ".csc-work-tenantID": "${fields.tenantID}",
        ".csc-work-displayName-label": {
            "messagekey": "work-displayNameLabel"
        },
        ".csc-work-genre": {
            "default": "",
            "optionnames": [
                "Please select a value",
                "Film",
                "Other"
            ],
            "optionlist": [
                "",
                "film",
                "other"
            ],
            "selection": "${fields.genre}"
