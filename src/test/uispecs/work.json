        },
        ".csc-work-tenantID-label": {
            "messagekey": "work-tenantIDLabel"
        },
        ".csc-work-status": "${fields.status}",
        ".csc-work-indexingNotes": "${fields.indexingNotes}",
        ".csc-work-creatorGroup": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
                    "options": {
                        "repeatTree": {
                            "expander": {
                                "tree": {
                                    ".csc-work-creator": {
                                        "decorators": [
                                            {
                                                "func": "cspace.autocomplete",
                                                "type": "fluid",
                                                "options": {
                                                    "queryUrl": "../../../tenant/core/vocabularies/work/autocomplete/creator",
                                                    "vocabUrl": "../../../tenant/core/vocabularies/work/source-vocab/creator"
                                                }
                                            }
                                        ],
                                        "value": "${{row}.creator}"
                                    },
                                    ".csc-work-creatorType": {
                                        "default": "",
                                        "optionnames": [
                                            "Please select a value",
                                            "Artist",
                                            "Director",
                                            "Publishing Company",
                                            "Developer",
                                            "Production Company"
                                        ],
                                        "optionlist": [
                                            "",
                                            "artist",
                                            "director",
                                            "publishingcompany",
                                            "developer",
                                            "productioncompany"
                                        ],
                                        "selection": "${{row}.creatorType}"
                                    }
                                },
                                "type": "fluid.noexpand"
                            }
                        },
                        "elPath": "fields.creatorGroup"
                    }
                }
            ]
        },
        ".csc-work-refName": "${fields.refName}",
        ".csc-work-createdAt": "${fields.createdAt}",
        ".csc-work-csid-label": {
            "messagekey": "work-csidLabel"
        },
        ".csc-work-uri-label": {
            "messagekey": "work-uriLabel"
        },
        ".csc-work-useFor": {
            "decorators": [
                {
                    "func": "cspace.makeRepeatable",
                    "type": "fluid",
